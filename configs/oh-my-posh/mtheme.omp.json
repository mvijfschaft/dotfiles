{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "time",
          "style": "plain",
          "foreground": "#007ACC",
          "properties": {
            "time_format": "15:04:05"
          }
        },
        {
          "type": "root",
          "style": "powerline",
          "foreground": "#111111",
          "background": "#ffff46",
          "properties": {
            "root_icon": "\uF0E7"
          }
        },
        {
          "type": "path",
          "style": "powerline",
          "foreground": "#56B6C2",
          "properties": {
            "home_icon": "\uF7DB",
            "folder_icon": "\uF115",
            "folder_separator_icon": " \uE0B1 ",
            "style": "mixed"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "powerline_symbol": "\uE0B0",
          "foreground": "#95ffa4",
          "properties": {
            "branch_icon": "\uE0A0 ",
            "branch_gone_icon": "\uD83D\uDC80",
            "display_status_detail": true,
            "status_colors_enabled": true,
            "display_status": true,
            "display_stash_count": true,
            "display_upstream_icon": true
          }
        },
        {
          "foreground": "#DCB977",
          "style": "plain",
          "template": " \uf119",
          "type": "exit"
        },
        {
          "type": "newline"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Env.TITLE }}",
  "final_space": true,
  "version": 2
}
