{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "root",
          "style": "powerline",
          "foreground": "#111111",
          "background": "#ffff46",
          "properties": {
            "root_icon": "\uF0E7"
          }
        },
        {
          "type": "session",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#ffffff",
          "background": "#303030",
          "properties": {
            "default_user_name": "test",
            "display_host": false,
            "display_default": false
          }
        },
        {
          "type": "text"
        },
        {
          "type": "path",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#100e23",
          "background": "#8db0c0",
          "properties": {
            "home_icon": "\uF7DB",
            "folder_icon": "\uF115",
            "folder_separator_icon": " \uE0B1 ",
            "style": "mixed"
          }
        },
        {
          "type": "text"
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "\uE0B0",
          "foreground": "#193549",
          "background": "#95ffa4",
          "properties": {
            "branch_icon": "\uE0A0 ",
            "status_colors_enabled": true,
            "ahead_color": "#99ff99",
            "behind_color": "#ff9999",
            "ahead_and_behind_color": "#ffff99"
          }
        }
      ]
    },
    {
      "type": "newline"
    }
  ],
  "console_title": true,
  "console_title_style": "template",
  "console_title_template": "{{ .Env.TITLE }}",
  "final_space": true
}
